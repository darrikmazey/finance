<%
	sc = account_item.class.all_superclasses
	@output = nil
	while @output.nil?
		c = sc.shift
		@output = render :partial => c.underscore, :object => account_item rescue nil
	end
%>

<%= @output %>

<%
	account_item.children.each do |child|
		sc = child.class.all_superclasses
		@output = nil
		while @output.nil?
			c = sc.shift
			@output = render :partial => c.underscore, :object => child rescue nil
		end
%>

<%= @output %>

<%
	end
%>
